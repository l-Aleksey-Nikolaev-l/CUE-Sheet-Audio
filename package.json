{
  "name": "cue-sheet-audio",
  "displayName": "CUE Sheet Audio",
  "description": "This is an extension for CUE Sheets of Audio files",
  "version": "0.2.0",
  "author": "Aleksey Nikolaev",
	"publisher": "",
	"repository": {
		"type": "git",
		"url": "https://github.com/l-Aleksey-Nikolaev-l/CUE-Sheet-Audio"
	},
	"icon": "images/icon.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cuesheet",
      "aliases": ["CUE Sheet Audio", "CUE Sheet", "cuesheet"],
      "extensions": [".cue",".CUE"],
      "configuration": "./cuesheet-configuration.json"
    }],
    "grammars": [{
      "language": "cuesheet",
      "scopeName": "source.cuesheet",
      "path": "./syntaxes/cuesheet.tmLanguage.json"
    }],
    "snippets": [{
				"language": "cuesheet",
				"path": "./snippets/snippets.json"
			}]
  },
  "configurationDefaults": {
    "[cuesheet]": {
      "workbench.editor.languageDetection": true,
      "editor.cursorWidth": 3,
      "editor.cursorBlinking": "phase",
      "editor.cursorSmoothCaretAnimation": "on",
      "editor.suggest.showWords": false,
      "editor.occurrencesHighlight": "off",
      "editor.fontSize": 16,
      "editor.smoothScrolling": true,
      "editor.semanticHighlighting.enabled": true
    }
  }
}
